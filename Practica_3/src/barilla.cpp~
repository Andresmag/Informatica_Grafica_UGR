#include "barilla.h"

//Constructor
Barilla::Barilla(){}

// Funcion para dibujar el cabezal en su sitio
voiBarilla::draw(char modo){
	glPushMatrix();
	glScalef(0.25,4.7,0.25);
	barilla.draw(modo);
	glPopMatrix();
}
